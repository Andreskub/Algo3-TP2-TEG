Object subclass: #AlgoVid	instanceVariableNames: 'personas burbujas colegios'	classVariableNames: ''	package: 'TP1-106051'!!AlgoVid methodsFor: 'initialization' stamp: 'S 5/19/2021 11:54'!initialize 	personas := Dictionary new.	burbujas := Dictionary new.	colegios := Dictionary new.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/15/2021 21:04'!agregarA: unNombrePersona aLaBurbuja: unNombreBurbuja	(self buscar: unNombreBurbuja en: burbujas )agregarA: (self buscar: unNombrePersona en: personas) ! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 13:54'!esPersonaDeRiesgo: nombrePersona 	(self buscar: nombrePersona en: personas) esDeRiesgo.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/15/2021 21:07'!estaVacunadx: unNombrePersona 	(self buscar: unNombrePersona en: personas) vacunar.		! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 14:22'!diagnosticoDe: unNombrePersona 	^(self buscar: unNombrePersona en: personas) devolverDiagnostico.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:54'!nombreDuplicado: unNombre en: unDiccionario	unDiccionario at: unNombre  ifPresent: [NombreDuplicado signal].! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 13:44'!puedeCircular: unNombrePersona	^((self buscar: unNombrePersona en: personas) devolverCirculacion).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/15/2021 21:05'!clasesPresencialesEn: unNombreColegio 	^(self buscar: unNombreColegio en: colegios) clasesPresenciales.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/15/2021 23:23'!contactoEstrechoEntre: nombrePrimerPersona y: nombreSegundaPersona	(self buscar: nombrePrimerPersona en: personas) agregarContacto: (self buscar: nombreSegundaPersona en: personas).	(self buscar: nombreSegundaPersona en: personas) agregarContacto: (self buscar: nombrePrimerPersona en: personas).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/15/2021 21:04'!agregarSintoma: unSintoma a: unNombrePersona  	(self buscar: unNombrePersona en: personas) agregarSintoma: unSintoma.	! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:53'!nuevoColegio: unNombreColegio	|unColegio|	unColegio := Colegio  new.	unColegio asignarNombre: unNombreColegio.	self nombreDuplicado: unNombreColegio en: colegios.	colegios at: unNombreColegio put: unColegio.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:53'!buscar: unNombreObjeto en: unDiccionario	^(unDiccionario at: unNombreObjeto ifAbsent:[NombreInexistente signal]).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 13:53'!esPersonalEsencial: nombrePersona	(self buscar: nombrePersona en: personas) esEsencial .	! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/15/2021 21:04'!agregarA:unNombreBurbuja alColegio: unNombreColegio	(self buscar: unNombreColegio en: colegios) agregarA: (self buscar: unNombreBurbuja en: burbujas)! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:53'!nuevaPersona: unNombrePersona	|unaPersona|	unaPersona := Persona new.	unaPersona asignarNombre: unNombrePersona.	self nombreDuplicado: unNombrePersona en: personas.	personas at: unNombrePersona put: unaPersona.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:53'!nuevaBurbuja: unNombreBurbuja	|unaBurbuja|	unaBurbuja := Burbuja  new.	unaBurbuja asignarNombre: unNombreBurbuja.	self nombreDuplicado: unNombreBurbuja en: burbujas. 	burbujas at: unNombreBurbuja put: unaBurbuja.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'S 5/20/2021 11:50'!burbujaPinchada: unNombreBurbuja	^(self buscar: unNombreBurbuja en: burbujas) estaPinchada.! !Object subclass: #Burbuja	instanceVariableNames: 'personas nombre estado'	classVariableNames: ''	package: 'TP1-106051'!!Burbuja methodsFor: 'as yet unclassified' stamp: 'S 5/20/2021 11:49'!agregarA: unaPersona 	personas add: unaPersona. 	unaPersona estaEnBurbuja: self.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/12/2021 00:39'!asignarNombre: unNombreBurbuja 	nombre := unNombreBurbuja ! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'a 6/20/2021 19:51'!pinchar	estado := Pinchada new.	! !!Burbuja methodsFor: 'as yet unclassified' stamp: 's 6/18/2021 14:45'!estaPinchada	personas detect:[:persona| persona esPositivo] ifFound: [self pinchar].	^estado estaPinchada.! !!Burbuja methodsFor: 'initialization' stamp: 'S 5/20/2021 11:50'!initialize	personas := OrderedCollection new.	estado := NoPinchada new.! !Object subclass: #BurbujaEstado	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!Object subclass: #Clases	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!Object subclass: #Colegio	instanceVariableNames: 'nombre presencialidad burbujas'	classVariableNames: ''	package: 'TP1-106051'!!Colegio methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 14:47'!clasesNoPresenciales	presencialidad := NoPresenciales new.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 14:43'!agregarA: burbuja 	burbujas add: burbuja.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/12/2021 12:51'!asignarNombre: unNombreColegio	nombre := unNombreColegio ! !!Colegio methodsFor: 'as yet unclassified' stamp: 's 6/18/2021 09:41'!clasesPresenciales	(burbujas isEmpty) ifTrue: [ColegioVacio signal].	(((burbujas count: [:x|x estaPinchada ])/burbujas size) > 0.4) ifTrue: [self clasesNoPresenciales].    ^presencialidad sonPresenciales.! !!Colegio methodsFor: 'initialization' stamp: 'S 5/19/2021 14:43'!initialize 	burbujas := OrderedCollection new.	presencialidad := Presenciales new.! !O