stamp: 'S 5/19/2021 12:21'!esHabitual	^(habituales includes: nombre). ! !Diagnostico subclass: #Sospechoso	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!Sospechoso methodsFor: 'accessing' stamp: 'S 5/19/2021 11:16'!nombre	^'Sospechoso'! !!Sospechoso methodsFor: 'as yet unclassified' stamp: 's 6/18/2021 09:27'!circulacion	^false.! !!Sospechoso methodsFor: 'as yet unclassified' stamp: 'a 6/20/2021 19:53'!esSospechoso	^true.! !!Sospechoso methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:29'!actualizarDiagnostico: sintomas de: unaPersona	(sintomas size = 3) ifTrue: [sintomas detect: [ :x| (x esHabitual)] ifFound: [unaPersona diagnosticoPositivo]].	((sintomas size >= 4)) ifTrue: [unaPersona diagnosticoPositivo].! !Object subclass: #Vacunada	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!Vacunada subclass: #PersonaNoVacunada	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!PersonaNoVacunada methodsFor: 'as yet unclassified' stamp: 's 6/18/2021 09:22'!vacuna	^false.! !Vacunada subclass: #PersonaVacunada	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!PersonaVacunada methodsFor: 'as yet unclassified' stamp: 's 6/18/2021 09:19'!vacuna	^true.! !