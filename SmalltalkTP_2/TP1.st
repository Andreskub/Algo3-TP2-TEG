estaEnBurbuja: unaBurbuja 	burbuja := unaBurbuja.! !!Persona methodsFor: 'mensajesPersona' stamp: 'a 6/20/2021 19:52'!devolverCirculacion	^(vacunacion vacuna | esencial esEsencial) & riesgo circulacion & diagnostico circulacion & (burbuja estaPinchada) not & (contactos anySatisfy: [:a| a esPositivo | a esSospechoso]) not.! !!Persona methodsFor: 'mensajesPersona' stamp: 's 6/18/2021 09:26'!esEsencial	esencial := PersonaEsencial new.! !!Persona methodsFor: 'mensajesPersona' stamp: 'S 5/19/2021 11:16'!devolverDiagnostico	^diagnostico nombre.! !!Persona methodsFor: 'mensajesPersona' stamp: 's 6/18/2021 14:37'!diagnosticoPositivo	diagnostico := Positivo new.	! !!Persona methodsFor: 'as yet unclassified' stamp: 's 6/18/2021 09:52'!esPositivo	^diagnostico esPositivo.! !!Persona methodsFor: 'as yet unclassified' stamp: 'a 6/20/2021 19:53'!esSospechoso	^diagnostico esSospechoso.! !!Persona methodsFor: 'initialization' stamp: 'a 6/20/2021 19:47'!initialize 	vacunacion := PersonaNoVacunada  new.	esencial := PersonaNoEsencial new.	diagnostico := Saludable new.	riesgo := PersonaNoRiesgo new.	burbuja := Burbuja new.	sintomas := OrderedCollection new.	contactos := OrderedCollection new.! !Esencial subclass: #PersonaEsencial	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!PersonaEsencial methodsFor: 'mensajesPersona' stamp: 's 6/18/2021 09:23'!esEsencial	^true.! !Esencial subclass: #PersonaNoEsencial	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!PersonaNoEsencial methodsFor: 'mensajesPersona' stamp: 's 6/18/2021 09:23'!esEsencial	^false.! !BurbujaEstado subclass: #Pinchada	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!Pinchada methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/12/2021 09:58'!estaPinchada 	^true! !Diagnostico subclass: #Positivo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!